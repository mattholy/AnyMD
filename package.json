{
  "name": "@mattholy/anymd",
  "version": "0.1.4",
  "description": "An open-source, customizable Markdown parser and renderer with Typescript.",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattholy/AnyMD"
  },
  "keywords": [
    "markdown",
    "activitypub",
    "parser",
    "renderer",
    "open-source",
    "typescript"
  ],
  "author": "Mattholy <smile.used@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "katex": "^0.16.11",
    "remark-directive": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/katex": "^0.16.7",
    "@types/unist": "^3.0.3",
    "@vitest/coverage-v8": "^2.1.5",
    "@vue/runtime-core": "^3.5.13",
    "jest-esm-transformer": "^1.0.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5",
    "vue": "^3.5.13"
  }
}
